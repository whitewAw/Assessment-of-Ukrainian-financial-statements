@inject IndicatorsOfEfficiencyOfUseOfIntangibleAssets IndicatorsOfEfficiencyOfUseOfIntangibleAssets

<RadzenCard Style="width: 100%;">
    <h2 class="text-center">@tableName</h2>
    <h5 class="text-end">(@ResLoc["thousandUAH"])</h5>
    <table class="table table-bordered py-0 table-sm table-striped">
        <THeadCommonComponent />
        <tbody>
            <TrTwoYearsCalculationDataComponent Item=@IndicatorsOfEfficiencyOfUseOfIntangibleAssets.NetIncomeFromSales>@ResLoc["NetIncomeFromSales"]</TrTwoYearsCalculationDataComponent>
            <TrTwoYearsCalculationDataComponent Item=@IndicatorsOfEfficiencyOfUseOfIntangibleAssets.AverageAnnualCostOfIntangibleAssets>@ResLoc["AverageAnnualCostOfIntangibleAssets"]</TrTwoYearsCalculationDataComponent>
            <TrTwoYearsCalculationDataComponent Item=@IndicatorsOfEfficiencyOfUseOfIntangibleAssets.ReturnOnIntangibleAssets Digits=2>@ResLoc["ReturnOnIntangibleAssets"], @ResLoc["currency"]</TrTwoYearsCalculationDataComponent>
            <TrTwoYearsCalculationDataComponent Item=@IndicatorsOfEfficiencyOfUseOfIntangibleAssets.CapitalIntensityOfProducts Digits=3>@ResLoc["CapitalIntensityOfProducts"], @ResLoc["currency"]</TrTwoYearsCalculationDataComponent>
        </tbody>
    </table>
</RadzenCard>

@code {
    private string? tableName;

    protected override void OnInitialized()
    {
        string name = ResLoc["IndicatorsOfEfficiencyOfUseOfIntangibleAssets"];
        int number = 9;

        tableName = String.Format("{0} {1}. {2} {3} {4}-{5} {6}", ResLoc["Table"], number, name, MainModel.CompanyName, MainModel.BaseYear, MainModel.CurrentYear, ResLoc["years"]);
        base.OnInitialized();
    }
}